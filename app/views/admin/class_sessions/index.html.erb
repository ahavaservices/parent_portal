<p id="notice"><%= notice %></p>

<h1>Listing Admin Class Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Start date</th>
      <th>End date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_class_sessions.each do |admin_class_session| %>
      <tr>
        <td><%= admin_class_session.title %></td>
        <td><%= admin_class_session.start_date %></td>
        <td><%= admin_class_session.end_date %></td>
        <td><%= link_to 'Show', admin_class_session %></td>
        <td><%= link_to 'Edit', edit_admin_class_session_path(admin_class_session) %></td>
        <td><%= link_to 'Destroy', admin_class_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Class session', new_admin_class_session_path %>
